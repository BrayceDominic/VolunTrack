{
  "expo": {
    "name": "VolunTrack",
    "slug": "VolunTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vt.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.braycedominic.voluntrack",
       "permissions": ["INTERNET"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vt.png",
          "resizeMode": "contain",
          "backgroundColor": "#D2B48C"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e3c5bd20-58dc-41f5-850d-03ac75c3b72e"
      }
    },
    "owner": "braycedominic11"
  }
}
