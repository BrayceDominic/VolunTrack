{
  "expo": {
    "name": "VolunTrack",
    "slug": "VolunTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vt.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.braycedominic.voluntrack"
      // "adaptiveIcon": {
      //   "foregroundImage": "./assets/images/adaptive-icon.png",
      //   "backgroundColor": "#ffffff"
      // }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
      "image": "./assets/images/volun2.webp",
      "resizeMode": "contain",
      "backgroundColor": "tan"
    }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
